h2#test ツイートを選択
= form_tag('/maps', :method=>'post')
  .tweets_list_box
    - @tweets.each_with_index do |tweet, idx|
      / - binding.pry
      .tweet_list_item
        label
          input type="checkbox" name="tweet[#{idx}]"
          = tweet[:oembed_html].html_safe

  input.ui.blue.button type="submit" value="Mapを作成"
